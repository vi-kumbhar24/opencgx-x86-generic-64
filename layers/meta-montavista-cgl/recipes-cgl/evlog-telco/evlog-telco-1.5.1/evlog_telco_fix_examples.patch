+/* POSIX 1003.25 Event Logging Test Application
+   David Brinkman
+
+   This program demonstrates how a kernel module can use the POSIX
+   Event Logging mechanism to write to the event log.
+*/
+
+/*
+ * Adapted for 2.6.x by Igor Goryachieff <igoryachev@ru.mvista.com>
+ */
+
+#include <linux/init.h>
+#include <linux/module.h>
+#include <linux/kernel.h>
+#include <linux/moduleparam.h>
+#include <linux/evl_log.h>
+#include <linux/evlog.h>
+
+//static char *str1 = "bar foo";
+static char str1[1024] = "bar foo";
+static char *MYFUNCTION = "printf";
+static int MYSEVERITY = 0;
+static int MYEVENTTYPE = 0;
+
+//module_param(str1, charp, 0);
+module_param_string(str1, str1, sizeof(str1), 0);
+module_param(MYFUNCTION, charp, 0);
+module_param(MYSEVERITY, int, 0);
+module_param(MYEVENTTYPE, int, 0);
+
+static int __init testmod_init(void)
+{
+	int  eventtype = MYEVENTTYPE;
+	int  severity = MYSEVERITY;
+	char *function = MYFUNCTION;
+
+	if(MYEVENTTYPE == 0)
+		eventtype=3;
+	else
+		eventtype=MYEVENTTYPE;
+	if(MYSEVERITY < 0 || MYSEVERITY > 7)
+		severity=3;
+	else
+		severity=MYSEVERITY;
+	if(MYFUNCTION == NULL)
+		strcpy(function, "printf");
+	else
+		strcpy(function, MYFUNCTION);
+
+	if(!strcmp(function, "printf"))
+		posix_log_printf(LOG_KERN, eventtype, severity, 0, "posix_log_printf testing %s", str1);
+
+	if(!strcmp(function, "write"))
+		posix_log_write(LOG_KERN, eventtype, severity, str1, strlen(str1) + 1,
+				POSIX_LOG_STRING, 0);
+
+	if(!strcmp(function, "printk"))
+	        evl_printk(LOG_KERN, eventtype, severity, "printk testing %s\n", str1);
+
+//        printk("printk testing - MYSEVERITY:%d MYEVENTTYPE:%d MYFUNCTION:%s str1:%s\n",MYSEVERITY,MYEVENTTYPE,MYFUNCTION, str1);
+//        posix_log_printf(LOG_KERN, eventtype, severity, 0, "posix_log_printf testing %s", str1);
+
+	return 0;
+}
+
+static void __exit testmod_exit(void)
+{
+	return;
+}
+
+module_init(testmod_init);
+module_exit(testmod_exit);
+
+MODULE_AUTHOR("Brinkman, David N <david.n.brinkman@intel.com>");
+MODULE_DESCRIPTION("A test module for posix event log");
+MODULE_LICENSE("GPL");
