+	@list='$(evlog_conf_DATA)'; for p in $$list; do \
+	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
+	  f="`echo $$p | sed -e 's|^.*/||'`"; \
+	  echo " $(evlog_confDATA_INSTALL) '$$d$$p' '$(DESTDIR)$(evlog_confdir)/$$f'"; \
+	  $(evlog_confDATA_INSTALL) "$$d$$p" "$(DESTDIR)$(evlog_confdir)/$$f"; \
+	done
