+include ../evlog_env
+KFACILITY=kern
+
+all: test_target pci.to
+
+test_target:
+	../test_target.sh $(KFACILITY)
+
+pci.to: pci.t
+	@sbindir@/evltc -I.. pci.t
+	$(ELA_BIN_DIR)/ela_get_atts -f $(KFACILITY) -d pci > pci.rules
