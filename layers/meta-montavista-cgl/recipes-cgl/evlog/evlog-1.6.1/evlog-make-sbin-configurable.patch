+
+diff $TESTNAME.out testfac2.tpl.out
+
+if [ $? -eq "0" ]
+then 
+	echo "testfac2	:PASSED"
+	@sbindir@/evlfacility -d TESTFAC2
+else
+	echo "testfac2	:FAILED"
+fi
